{
  "name": "@rarible/protocol-api-client",
  "version": "0.1.1",
  "keywords": [
    "axios",
    "typescript",
    "openapi",
    "openapi-generator",
    "api_documentation"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "typings.d.ts"
  ],
  "scripts": {
    "download": "rimraf ./api-docs && download $API_SPEC_PATH --out=\"./api-docs\"",
    "generate": "rimraf ./src && openapi-generator-cli generate && rimraf ./src/models && mvn -f model/pom.xml clean model-generator:generate && move-file model/target/models ./src/models",
    "build": "rimraf ./build && tsc"
  },
  "dependencies": {
    "@rarible/types": "0.1.3"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.3.6",
    "download-cli": "1.1.1",
    "move-file-cli": "2.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  }
}
